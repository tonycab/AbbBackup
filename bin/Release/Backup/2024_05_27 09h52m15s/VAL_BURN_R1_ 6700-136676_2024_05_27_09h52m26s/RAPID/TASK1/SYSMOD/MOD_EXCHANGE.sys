MODULE MOD_EXCHANGE(SYSMODULE)

    PROC Motor_1(\switch ON|switch OFF)

        IF OpMode()=OP_MAN_PROG RETURN ;

        IF Present(ON) set OR_29;
        IF Present(OFF) reset OR_29;

    ENDPROC

    !---------------------------------------------------------------------------------------------------
    PROC Motor_2(\switch ON|switch OFF)

        IF OpMode()=OP_MAN_PROG RETURN ;

        IF Present(ON) set OR_30;
        IF Present(OFF) reset OR_30;

    ENDPROC

    !---------------------------------------------------------------------------------------------------
    PROC Motor_3(\switch ON|switch OFF)

        IF OpMode()=OP_MAN_PROG RETURN ;

        IF Present(ON) set OR_31;
        IF Present(OFF) reset OR_31;

    ENDPROC

    !---------------------------------------------------------------------------------------------------
    PROC Motor_4(\switch ON|switch OFF)

        IF OpMode()=OP_MAN_PROG RETURN ;

        IF Present(ON) set OR_32;
        IF Present(OFF) reset OR_32;

    ENDPROC

    !***************************************************************************************************
    !Robot dans la zone table
    PROC Zone_table(\switch Inside|switch OutSide)
        IF Present(OutSide) Set OR_06;
        IF Present(Inside) Reset OR_06;
    ENDPROC
    
    !Robot dans la zone table
    PROC Zone_satellite(\switch Inside|switch OutSide)
        IF Present(OutSide) Set OR_08;
        IF Present(Inside) Reset OR_08;
    ENDPROC


ENDMODULE